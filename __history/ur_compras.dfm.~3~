  object Q_dados: TFDQuery
    Connection = DM.FDConnection1
    SQL.Strings = (
      'select c.cod_compra,'
      #9#9'c.nota_fiscal,'
      #9#9'c.data,'
      #9#9'ic.valor_unitario as valor_unitario_ingrediente,'
      #9#9'ic.quantidade,'
      #9#9'i.descricao as ingrediente,'
      #9#9'f.razao_social as fornecedor,'
      #9#9'u.descricao as unidade,'
      #9#9'i.cod_ingrediente,'
      #9#9'c.valor_total as valor_total_ingrediente'
      'from compras as c'
      
        #9'inner join fornecedores as f on (f.cod_cidade = c.cod_fornecedo' +
        'r)'
      #9'inner join itens_compra as ic on (ic.cod_compra = c.cod_compra)'
      
        #9'inner join ingredientes as i on (i.cod_ingrediente = ic.cod_ing' +
        'rediente)'
      #9'inner join unidades as u on (u.cod_unidade = i.cod_unidade)'
      'order by nota_fiscal,ingrediente')
    Left = 326
    Top = 382
  end
end
