  object q_ingredientes: TFDQuery
    Active = True
    Connection = DM.FDConnection1
    SQL.Strings = (
      'select i.cod_ingrediente,'
      '       i.valor_unitario,'
      '       i.descricao,'
      '       u.descricao as unidade'
      'from ingredientes as i'
      'inner join unidades as u on (i.cod_unidade = u.cod_unidade)')
    Left = 208
    Top = 232
    object q_ingredientescod_ingrediente: TFDAutoIncField
      FieldName = 'cod_ingrediente'
      Origin = 'cod_ingrediente'
      ProviderFlags = [pfInWhere, pfInKey]
      ReadOnly = True
    end
    object q_ingredientesvalor_unitario: TFloatField
      FieldName = 'valor_unitario'
      Origin = 'valor_unitario'
      DisplayFormat = '###,##0.00'
    end
    object q_ingredientesdescricao: TStringField
      FieldName = 'descricao'
      Origin = 'descricao'
      Required = True
      Size = 100
    end
    object q_ingredientesunidade: TStringField
      FieldName = 'unidade'
      Origin = 'unidade'
      Required = True
      Size = 100
    end
  end
